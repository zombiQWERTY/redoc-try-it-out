{
  "compilerOptions": {
    "module": "ES6",
    "moduleResolution": "classic",
    "target": "ES6",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "baseUrl": "src",
    "outDir": "dist",
    "removeComments": true,
    "sourceMap": true,
    "noImplicitAny": true,
    "lib": [
      "dom",
      "es5",
      "es6",
      "es2017"
    ]
  }
}
